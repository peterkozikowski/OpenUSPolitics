    "react-dom": "^19.2.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.0",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.37.0",
    "eslint-config-next": "15.5.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.4.5",
    "wrangler": "^4.42.1"
  }
}
